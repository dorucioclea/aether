    "jquery": "~3.2.0",
    "jsdom": "~11.2.0",
    "mocha": "~3.5.0",
    "moment": "~2.18.0",
    "nock": "~9.0.0",
    "node-fetch": "~1.7.0",
    "node-sass": "~4.5.0",
